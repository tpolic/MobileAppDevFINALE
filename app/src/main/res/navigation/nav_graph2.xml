<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph2"
    app:startDestination="@id/eventFragment">

    <fragment
        android:id="@+id/eventFragment"
        android:name="com.example.finale.view.EventFragment"
        android:label="fragment_event_list"
        tools:layout="@layout/fragment_event_list" >

    </fragment>
    <fragment
        android:id="@+id/detailEventFragment"
        android:name="com.example.finale.view.detail.DetailEventFragment"
        android:label="fragment_detail_event"
        tools:layout="@layout/fragment_detail_event">
        <argument
            android:name="event"
            app:argType="com.example.finale.data.model.Event" />
    </fragment>
    <action
        android:id="@+id/action_eventFragment_to_detailEventFragment"
        app:destination="@id/detailEventFragment" />
</navigation>